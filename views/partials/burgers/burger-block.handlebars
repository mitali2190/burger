<div class="card" >
	<div class="card-body">
		<h5 class="card-title">{{burger_name}}</h5>
		<button class="btn btn-primary change-devoured" data-id="{{id}}" data-newdevoured="{{devoured}}">
			{{#if devoured}}DEVOURED!{{else}}EAT!{{/if}}
		</button>

		<button class="btn btn-danger delete-burger" data-id="{{this.id}}">DELETE!</button>
	</div>
</div>